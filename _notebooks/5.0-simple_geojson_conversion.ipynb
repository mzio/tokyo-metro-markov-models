    "def timestamp_to_json(obj):\n",
    "    if isinstance(obj, pd.datetime):\n",
    "        return obj.__str__()\n",
    "    \n",
    "def row_to_geojson(row, json_list=[]):\n",
    "    dict = {\"type\": \"Feature\",\n",
    "            \"geometry\": {\n",
    "                \"type\": \"Point\",\n",
    "                \"coordinates\": [row['longitude'], row['latitude']]\n",
    "            },\n",
    "            \"properties\": {\n",
    "                \"local_time\": row['localTimestamp'],\n",
    "                \"line_color\": row['line_color'],\n",
    "            }}\n",
    "    json_list.append(dict)"
   ]
  },
